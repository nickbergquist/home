{
  "publish": "full",
  "paths": {
    "stylesheets": {
      "scss": "./Assets/sass/**/*.scss",
      "css": "./wwwroot/css",
      "ignore": "!./Assets/sass/themes/*",
      "themes": "./Assets/sass/themes/",
      "conditionalVendor": "./Assets/css/conditional/*.css"
    },
    "javascript": {
      "srcMain": "./Assets/js/main/**/*.js",
      "mainSite": "./Assets/js/main/site.js",
      "srcStatic": "./Assets/js/static/",
      "srcConditional": "./Assets/js/conditional/**/*.js",
      "srcConditionalIgnore": "!./Assets/js/conditional/**/*.fwibble",
      "dest": "./wwwroot/js",
      "destAngularAppFiles": "./wwwroot/js/appfiles",
      "generalSiteLintConfig": "./Assets/js/main/.eslintrc.js"
    }
  }
}